; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Firefight"
config/description="A small Godot 2D demo, where you play top-down trying to save the forest from fire."
run/main_scene="res://scenes/main/main.tscn"
config/features=PackedStringArray("4.3", "Mobile")
run/delta_smoothing=false
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://assets/branding/icon.svg"

[display]

window/size/viewport_width=1024
window/size/viewport_height=1024
window/stretch/mode="viewport"
window/vsync/vsync_mode=0
window/defaults/default_clear_color=Color(0.284339, 0.000411422, 0.798572, 1)

[rendering]

renderer/rendering_method="mobile"
